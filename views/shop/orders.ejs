<%- include('../includes/head.ejs')%>
<link rel="stylesheet" href="/css/product.css">
</head>
<body>
    <%- include('../includes/navigation.ejs')%>
    <main>
        <% if(orders.length <=0) {%>
            There is no orders.
        <%} else {%>
            <% orders.forEach(order => { %>
                <h1>#<%= order.id %></h1>
                 <ul>
                     <% order.products.forEach(product => { %>
                        <li> <%= product.title %> (<%= product.orderitem.quantity %>) </li>
                     <% }); %>
                 </ul>

            <% }); %>
        <%}%>
    </main>
</body>